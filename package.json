  "author": "Alexander Davies",
  "license": "ISC",
  "engines": {
    "node": ">=10.15.0"
  },
  "scripts": {
    "test": "jest src/**/*.js",
    "lint": "eslint src/**/*.js --fix",
    "format": "prettier --write 'src/**/*.js'"
  },
  "dependencies": {
    "lodash": "^4.17.20"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.13.0",
    "express": "^4.17.1",
    "jest": "^26.5.3",
    "prettier": "^2.1.2",
    "supertest": "^5.0.0"
  }
}