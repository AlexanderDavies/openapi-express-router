{
  "name": "openapi-express-router",
  "version": "1.0.6",
  "description": "A simple package to build and connect Express routes based on an OpenAPI 3 or Swagger 2 JSON spec",
  "main": "src/index.js",
  "keywords": [
    "OpenAPI",
    "3.0",
    "Express",
    "Swagger",
    "Router",
    "Middleware",
    "Validation"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/AlexanderDavies/openapi-express-router"
  },
  "author": "Alexander Davies",
  "license": "ISC",
  "engines": {
    "node": ">=10.22.0"
  },
  "scripts": {
    "test": "jest src/**/*.js",
    "lint": "eslint src/**/*.js --fix",
    "format": "prettier --write 'src/**/*.js'",
    "docs": "jsdoc src -r -d docs",
    "prepare": "husky install"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.2.0",
    "express": "^4.17.1",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "jsdoc": "^3.6.6",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3"
  },
  "types": "types.d.ts"
}
